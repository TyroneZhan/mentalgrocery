@{
    ViewBag.Title = "Home Page";
}
<!-- Start hero_bg section -->
<section class="hero_bg hero_v1 bg_image" style="        background-image: url(assets/images/hero_bg1.jpg);">
    <div class="bg_overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <div class="banner_content">
                    <div class="domar_content_box">
                        <h1>Find activities you interested</h1>
                        <p>Step out of your home and be a member of one group<br>having fun having friends</p>
                    </div>
                    <div class="domar_button">
                        <a href="#" class="domar_btn">Join Us</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section><!-- End hero_bg section -->
<!-- Start domar_countdown section -->
<section class="domar_countdown">
    <div class="domar_offer_box">
        <div class="row align-items-center">
            <div class="col-lg-5">
                <div class="countdown_newsletter">
                    <h4>Comming activity: <span>Active name</span></h4>
                    <form>
                        <div class="form_group">
                            <input type="email" class="form_control" placeholder="Enter your E-mail" name="email">
                            <button class="domar_btn">Subscribe</button>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-lg-7">
                <div class="offer_countdown_area text-center">
                    <div class="offer_countdown" data-countdown="2020/03/21"></div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End domar_countdown section -->
